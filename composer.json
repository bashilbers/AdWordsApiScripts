{
    "name": "bashilbers/AdWordsApiScripts",
    "description": "Single-purpose scripts for dealing with everyday problems in AdWords (using the API)",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sebastiaan Hilbers",
            "email": "bas.hilbers@adchieve.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "1.0",
        "guzzle/guzzle": "3.*",
        "wyrihaximus/react-guzzle-ring": "1.*",
        "googleads/googleads-php-lib": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "monolog/monolog": "1.*"
    },
    "autoload": {
        "psr-4": {
            "AdWordsApiScripts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdWordsApiScripts\\Tests\\": "tests/"
        }
    }
}
